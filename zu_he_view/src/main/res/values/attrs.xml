<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingImageView">
        <!--attr:表示属性
        name:属性名称,随便搞
        format:boolearn,string,color.....
        enum:限定传入的对象
        写完这个之后记得同步一下-->
        <attr name="my_background" format="enum">
            <enum name="first" value="0" />
            <enum name="middle" value="1" />
            <enum name="last" value="2" />
        </attr>
        <attr name="my_title" format="string" />
        <attr name="my_istoggle" format="boolean" />
    </declare-styleable>
</resources>